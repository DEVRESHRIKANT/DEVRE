@*@using System.Web.Optimization*@
<!DOCTYPE HTML>
<html>
<head>
    <title>@ViewBag.Title - Shrikant Profile</title>
    <meta name="viewport" charset="utf-8" content="width=device-width" initial-scale="1.0" />
    <script src="http://code.jquery.com/jquery.js"></script>
    <link href="~/Content" rel="stylesheet" media="screen" />
    <script src="~/Scripts/bootstrap.min.js"></script>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" media="screen" />
    @Styles.Render("/Content")
    @Scripts.Render("/Scripts")

    <style type="text/css">
        ul {
            list-style: none;
            font-size: large;
        }

        li {
        }

        ul li {
            display: block;
            position: relative;
            float: left;
            border: 1px solid #000;
        }

        li ul {
            display: none;
        }

        ul li a {
            display: block;
            padding: 5px 10px 5px 10px;
            text-decoration: none;
            white-space: nowrap;
            color: #ff6a00;
        }

            ul li a:hover {
                background: yellow;
            }

        li:hover ul {
            display: block;
            position: absolute;
        }

        li:hover li {
            float: none;
            margin: 0px;
        }

        li:hover a {
        }

        li:hover li a:hover {
            background: yellow;
        }

        #drop-nav li ul li {
            border-top: 0px;
        }
    </style>
</head>
<body>
    <div id="ShreeRightBar" style="width:auto"  cellpadding="0" cellspacing="0">
        <ul style="text-align:center; background-color:aqua; font-size:medium">
            <li style="text-align:left; background-color:aqua; font-size:medium;  border:none">
                <a href="#" style="font-family:Arial; font-size:larger; background-color:coral; color:firebrick">Knowledge</a>
                @Html.ActionLink("EngagementModels", "EngagementModels", "ShrikantDevre")
                @Html.ActionLink("Automation Frame", "AutomationFrame", "ShrikantDevre")
                @Html.ActionLink("Cloud Computing", "CloudComputing", "ShrikantDevre")
                @Html.ActionLink("InfrastructureMgmt", "InfrastructureMgmt", "ShrikantDevre")
                @Html.ActionLink("Build Server", "BuildServer", "ShrikantDevre")
                @Html.ActionLink("Business Analyst", "BusinessAnalyst", "ShrikantDevre")
                @Html.ActionLink("C# Sharp", "CSharp", "ShrikantDevre")
                @Html.ActionLink("PM Templates", "PM_Templates", "ShrikantDevre")
                @Html.ActionLink("Infras. Mgmt", "InfrastructureMgmt", "ShrikantDevre")
                @Html.ActionLink("QA Templates", "QA_Templates", "ShrikantDevre")
                @Html.ActionLink("Continuous Int.", "ContinuousIntegration", "Home")
                @Html.ActionLink("Risk Management", "RiskMgmt", "ShrikantDevre")
                @Html.ActionLink("Sharepoint", "SharePoint", "ShrikantDevre")
                @Html.ActionLink("Data Warehouse", "DataWarehouse", "ShrikantDevre")
                @Html.ActionLink("Quality Assurance", "QualityAssurance", "ShrikantDevre")
                @Html.ActionLink("Project Charter", "ProjectCharter", "ShrikantDevre")
            </li>
        </ul>
    </div>
</body>
</html>